[flake8]
max-line-length = 120
ignore = E203, W503, F401, F541, F811, F841, F402, E712
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .tox,
    dist,
    build,
    *.egg-info,
    tests/uat/
per-file-ignores =
    __init__.py:F401
    test_*.py:F401,F541,F811,F841,F402,E712
    tests/*:F401,F541,F811,F841,F402,E712